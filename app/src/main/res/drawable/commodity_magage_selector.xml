<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#E6E6E6"></solid>

            <!--<corners android:bottomLeftRadius="6dp" android:topLeftRadius="@dimen/dimen_6dp"></corners>-->

            <!-- 描边 -->
            <stroke android:width="1dp" android:color="#ffffff" android:dashWidth="2dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <solid android:color="@color/white"></solid>
            <!--<corners android:bottomLeftRadius="@dimen/dimen_6dp" android:topLeftRadius="@dimen/dimen_6dp"></corners>-->
            <stroke android:width="1dp" android:color="#ffffff" android:dashWidth="2dp" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <solid android:color="#E6E6E6"></solid>
            <!--<corners android:bottomLeftRadius="@dimen/dimen_6dp" android:topLeftRadius="@dimen/dimen_6dp"></corners>-->
            <stroke android:width="1dp" android:color="#ffffff" android:dashWidth="2dp" />
        </shape>
    </item>
</selector>